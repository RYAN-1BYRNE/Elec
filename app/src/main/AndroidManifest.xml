<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.electronicstore">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application



        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ryan.electronicstore.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ryan.electronicstore.Home" />
        <activity android:name="com.example.ryan.electronicstore.Cart" />
        <activity android:name="com.example.ryan.electronicstore.ProductList" />
        <activity android:name="com.example.ryan.electronicstore.SignUp" />
        <activity android:name="com.example.ryan.electronicstore.SignIn" />
        <activity android:name="com.example.ryan.electronicstore.ProductDetail" />
        <activity android:name="com.example.ryan.electronicstore.OrderDetail" />
        <activity android:name="com.example.ryan.electronicstore.OrderStatus"></activity>
    </application>

</manifest>